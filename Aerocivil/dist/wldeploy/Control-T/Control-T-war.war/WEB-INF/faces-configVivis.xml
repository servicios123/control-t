<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <application>
        <view-handler>
            com.sun.facelets.FaceletViewHandler
        </view-handler>    
        <message-bundle>co.gov.aerocivil.controlt.web.resources.errores_es</message-bundle>
        <locale-config>
            <default-locale>es</default-locale>
            <supported-locale>es</supported-locale>
        </locale-config>        
        <resource-bundle>
            <base-name>/messages</base-name>
            <var>msg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>logedIn</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editarRegional</from-outcome>
            <to-view-id>/Regional/EditRegional.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>listarRegional</from-outcome>
            <to-view-id>/Regional/ListarRegional.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editarAeropuerto</from-outcome>
            <to-view-id>/Aeropuerto/EditAeropuerto.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>listarAeropuerto</from-outcome>
            <to-view-id>/Aeropuerto/ListarAeropuerto.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>editarDependencia</from-outcome>
            <to-view-id>/Dependencia/EditDependencia.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>listarDependencia</from-outcome>
            <to-view-id>/Dependencia/ListarDependencia.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editarFuncionario</from-outcome>
            <to-view-id>/Funcionario/EditFuncionario.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>listarFuncionario</from-outcome>
            <to-view-id>/Funcionario/ListarFuncionario.xhtml</to-view-id>
        </navigation-case>
    <navigation-case>
            <from-outcome>editarFuncion</from-outcome>
            <to-view-id>/Funcion/EditFuncion.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>listarFuncion</from-outcome>
            <to-view-id>/Funcion/ListarFuncion.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>editarSector</from-outcome>
            <to-view-id>/Sector/EditSector.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>listarSector</from-outcome>
            <to-view-id>/Sector/ListarSector.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
